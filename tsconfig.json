{
   "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "noImplicitAny": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "*": [
          "node_modules/*"
        ]
      },
      
      "esModuleInterop": true
   },
   "include": [
     "src/**/*"
   ]
}